package model;

public enum StatusAgendamento {
	AGENDAMENTO_PENDENTE("Agendamento Pendente"), AGENDAMENTO_CONCLUIDO("Agendamento Concluido"),
	AGENDAMENTO_NAO_CONCLUIDO("Agendamento NÃ£o Concluido"), AGENDAMENTO_CANCELADO("Agendamento Cancelado"), CONCLUIDO("Agendamento Concluido");

	private String resumo;

	private StatusAgendamento(String resumo) {
		this.resumo = resumo;
	}

	public String getResumo() {
		return resumo;
	}
}